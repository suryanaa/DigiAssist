             />
           <Button android:layout_height="25dip" 
            android:layout_width="25dip"  
            android:background="@drawable/helpb" 
            android:layout_toLeftOf="@+id/textView6" 
            android:gravity="center"></Button>
    </TableRow>
    </TableLayout>
<TableLayout
    android:id="@+id/tableLayouttrial"
