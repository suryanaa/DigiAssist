<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" 
				android:id="@+id/relativeLayoutMain" android:layout_width="fill_parent" 
				android:layout_height="fill_parent" android:gravity="fill"
				android:background="@color/background">

<RelativeLayout android:id="@+id/headerLayout" android:layout_height="wrap_content" 
				android:padding="8dip" android:paddingBottom="8dip" android:paddingLeft="8dip" android:paddingRight="8dip" 
				android:layout_width="wrap_content" android:layout_alignParentTop="true">
<TableLayout
    android:id="@+id/tableLayout1"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content" 
    android:layout_alignParentTop="true"
    >
    <TableRow
        android:id="@+id/tableRow1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        >
	   <ImageView
  	  android:id="@+id/imageHealth"
	  android:layout_width="50dip"
	  android:layout_height="50dip" android:src="@drawable/images" android:layout_alignParentLeft="true"
	  />
	    <TextView
	     android:id="@+id/maintextView1"
	     android:text="Medical Measurements"
	     android:textSize="20dp" android:textColor="#000066"
	     android:typeface="serif" android:textStyle="bold"
	     android:textAppearance="?android:attr/textAppearanceMedium" 
	     android:paddingLeft="15px"
	     android:paddingTop="0px"/>        
    </TableRow>
</TableLayout>
<TableLayout
    android:id="@+id/tableLayout6"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content" 
    android:layout_alignParentTop="true"
    android:paddingTop="50px"
    >
    <TableRow
        android:id="@+id/tableRow2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        >
 
        <TextView
            android:id="@+id/PatientNamefirst"
            android:text="Name of Patient: "
            android:textSize="15dp" android:textColor="#000066"
	        android:typeface="serif" android:textStyle="bold"
            android:textAppearance="?android:attr/textAppearanceMedium" 
            android:paddingTop="15px"
           
            />
        
        <TextView
            android:id="@+id/textView20"
            android:text="Ms. Luis(ID:4567)"
            android:textSize="15dp" android:textColor="#000000"
	        android:typeface="serif" android:textStyle="bold"
            android:textAppearance="?android:attr/textAppearanceMedium" 
            android:paddingTop="25px"
            android:paddingLeft="2px"
            />
        
 </TableRow>
</TableLayout>    

</RelativeLayout>
<LinearLayout 
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    android:paddingTop="20px"
    >
<TableLayout
    android:id="@+id/tableLayout2"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:paddingTop="15px"
    >

 <!-- 2 columns -->
    <TableRow
        android:id="@+id/tableRow2"
        android:layout_height="fill_parent"
        android:layout_width="wrap_content"
        android:paddingTop="85px"
        
      >
 
        <TextView
            android:id="@+id/textView5"
            android:text="Resp Rate:"
            android:textSize="15dp" android:textColor="#000066"
	        android:typeface="serif" android:textStyle="bold"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:paddingLeft="5px"
            android:paddingRight="10px"
            android:gravity="right"
            
             />
 
        <EditText
            android:id="@+id/respText"
            android:text="16" 
            android:layout_width="80dp"
            android:layout_height="40dip"
            android:textSize="15px"
            

            />
 
        <TextView
           android:id="@+id/textView6"
            android:text="bpm"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textSize="15dp" android:textColor="#000000"
            android:textStyle="bold"
            android:paddingLeft="6dip"
            android:paddingRight="10dip"
             />
             
            <Button android:layout_height="25dip" 
            android:layout_width="25dip"  
            android:background="@drawable/helpb" 
            android:layout_toLeftOf="@+id/textView6" 
            android:gravity="center"></Button>
     </TableRow>
 </TableLayout>
 
 <TableLayout
    android:id="@+id/tableLayouttemp"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content" 
    android:layout_alignParentTop="true"
    >
   <TableRow 
        android:id="@+id/tableRow5"
        android:layout_height="wrap_content">
       <TextView
           android:id="@+id/Dummy"
            android:text="" 
            android:textColor="#FF0000"
            android:textStyle="italic"
            android:paddingRight="90px" />
       <TextView
           android:id="@+id/errorRespTextView"
            android:text="Normal range: 12-18bpm" 
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textSize="14dp" 
            android:textStyle="italic"
            android:textColor="#000000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
           
 
    </TableRow>
         <TableRow 
        android:id="@+id/tableRowA"
        android:layout_height="wrap_content">
       <TextView
           android:id="@+id/errorRespTextView1"
            android:text="" 
