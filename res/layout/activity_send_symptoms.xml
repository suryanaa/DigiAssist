        android:layout_alignParentLeft="true"
        android:layout_below="@+id/chkBoxRunny_nose"
        android:layout_marginTop="19dp"
        android:text="@string/diarrhea" />
      
    <CheckBox
        android:id="@+id/chkBoxDepressed"
        android:layout_width="wrap_content"
